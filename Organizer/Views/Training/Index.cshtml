@model Organizer.Models.ViewModels.TraningViewModel

@{

    ViewData["Title"] = "Training Page";
}


<h1>Training Page is here</h1>

<table>
    <tr>
        <th>Name</th>
        <th>Value</th>
    </tr>

    @foreach (var item in Model.TraningValues)
    {
        <tr>
            <td>@Html.DisplayFor(i => item.Name)</td>
            <td>@Html.DisplayFor(i => item.Value)</td>
        </tr>
    }

</table>




